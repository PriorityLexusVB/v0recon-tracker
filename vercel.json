{
  "buildCommand": "npm install --force && prisma generate && prisma db push && next build",
  "installCommand": "npm install --force",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
  }
}
